<% unless nb_exposes_total.nil? %>
  <div class="box noir">
    <div class="box-in box-front noir">
      <div class="flip-card-front noir">
        <div id="flexible">
          <div class="c100 p<%= nb_femmes_age.fdiv(nb_exposes_total).to_i * 100 %> noir">
            <span><%= nb_femmes_age.fdiv(nb_exposes_total).to_i * 100 %>%</span>
            <div class="slice">
              <div class="bar"></div>
              <div class="fill"></div>
            </div>
          </div>
        </div>
        <div class="card-organization-numbers">
          <div class="card-organization-name" style="display: flex; align-items: center; justify-content:center;">
            <p class="paragraphe-bold"><%= title %></p>
          </div>
        </div>
      </div>
    </div>
  </div>
<% end %>
